{
  "_version": "1.12.0",
  "sap.app": {
    "id": "sap.michielh.legoarm",
    "type": "application",
    "i18n": {
      "bundleUrl": "i18n/i18n.properties",
      "supportedLocales": ["en"],
      "fallbackLocale": "en"
    },
    "applicationVersion": { "version": "0.0.2" },
    "title": "{{appTitle}}",
    "description": "{{appDescription}}"
  },

  "sap.ui": {
    "technology": "UI5",
    "icons": {},
    "deviceTypes": { "desktop": true, "tablet": true, "phone": true }
  },

  "sap.ui5": {
    "flexEnabled": false,
    "rootView": {
      "viewName": "sap.michielh.legoarm.view.MainView",
      "type": "XML",
      "async": true,
      "id": "MainView"
    },
    "dependencies": {
      "minUI5Version": "1.65.6",
      "libs": {
        "sap.ui.core": {},
        "sap.m": {},
        "sap.ui.layout": {},
        "sap.dm.dme.podfoundation": {}
      }
    },
    "contentDensities": { "compact": true, "cozy": true },
    "models": {
      "i18n": {
        "type": "sap.ui.model.resource.ResourceModel",
        "settings": { "bundleName": "sap.michielh.legoarm.i18n.i18n" }
      }
    },
    "resources": {
      "css": [{ "uri": "css/style.css" }],
      "js": []
    }
  }
}
